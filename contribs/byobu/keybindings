source /usr/share/byobu/keybindings/f-keys
escape ^Ww
register r "^w:source $HOME/.byobu/profile^M"           #     | Goes with F5 definition
register t "^wA^aa^k^h"                                 #     | Goes with the F8 definition
register s "^w[g G$>^h"                                 # Goes with ^a~ definition
register d "^w:source /usr/share/byobu/keybindings/screen-escape-keys^M"
register r "^w:source $HOME/.byobu/profile^M"
register e "^w:source /usr/share/byobu/keybindings/f-keys^M"
